{
  "expo": {
    "name": "Couples Workout",
    "slug": "couples-workout",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "couples-workout",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "Track workouts & habits together as a couple. Stay motivated, stay connected.",
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.emergent.couplesworkout",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.emergent.couplesworkout",
      "versionCode": 1,
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000"
        }
      ],
      "expo-secure-store",
      "expo-apple-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "couples-workout-app"
      }
    }
  }
}